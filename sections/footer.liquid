<footer class="footer-container footer-layout-1 container {% if  section.settings.enable_footer_fullwidth  %}full-width {%endif %}" style="padding:{{ section.settings.layout_footer }}; background-color:white; width:100%">
  {% unless section.settings.enable_footer_fullwidth %}
  <div class="container">
    {% endunless  %}
    <div class="footer-top" style="width:100%;  font-family: 'Montserrat', sans-serif; font-size: 10px;">
      <div class="row" style="padding: 3% 0% 1% 0%">
        <div class="footer-top-col-group">
          <div class="col-md-2 col-sm-6 col-xs-12 oaki-logo-width">
            <aside id="custom_html-2" class="widget_text widget widget_custom_html" style="width: 80%; margin-bottom: 40px;">
              <div class="textwidget custom-html-widget">
                <div class="footer-logo">
                  <img src="https://cdn.shopify.com/s/files/1/2571/3660/files/oaki-logo-green.jpg?7163930291748246036">
                </div>
                <div class="row social-media-container">
                  <div class="col-md-4 col-sm-4 col-xs-4">
                    <span class="footer-si">
                      <a href="https://www.facebook.com/oakiwear/">
                        <!-- <img src="https://cdn.shopify.com/s/files/1/2571/3660/files/social-fb.png?12963397309132350615"> -->
                        <i class="fab fa-facebook oaki-facebook"></i>
                      </a>
                    </span>
                  </div>
                  <div class="col-md-4 col-sm-4 col-xs-4">
                    <span class="footer-si">
                      <a href="https://www.instagram.com/oakiwear/">
                        <!-- <img src="https://cdn.shopify.com/s/files/1/2571/3660/files/social-instagram.png?12963397309132350615"> -->
                        <i class="fab fa-instagram oaki-instagram"></i>
                      </a>
                    </span>
                  </div>
                  <div class="col-md-4 col-sm-4 col-xs-4">
                    <span class="footer-si">
                      <a href="https://www.pinterest.com/oakipin/?eq=oaki&amp;etslf=1814">
                        <!-- <img src="https://cdn.shopify.com/s/files/1/2571/3660/files/social-pinterest.png?12963397309132350615"> -->
                        <i class="fab fa-pinterest-square oaki-pinterest"></i>
                      </a>
                    </span>
                  </div>
                </div>
                <div class="contact-info-container">
                  <div class="col-md-12 col-sm-12 col-xs-12">
                    <p>
                      p: 385-515-2640
                    </p>
                    <p>
                      e: info@oaki.com
                    </p>
                  </div>
                </div>
              </div>
            </aside>
            </div>
          </div>
          {% for block in section.blocks %}
          {%- case block.settings.width -%}
          {% when '16%' %}
          {%- assign class_width = 'col-md-2' -%}
          {% when '25%' %}
          {%- assign class_width = 'col-md-3' -%}
          {% when '34%' %}
          {%- assign class_width = 'col-md-4' -%}
          {% when '42%' %}
          {%- assign class_width = 'col-md-5' -%}
          {% when '50%' %}
          {%- assign class_width = 'col-md-6' -%}
          {% when '58%' %}
          {%- assign class_width = 'col-md-7' -%}
          {% when '66%' %}
          {%- assign class_width = 'col-md-8' -%}
          {% when '75%' %}
          {%- assign class_width = 'col-md-9' -%}
          {% when '84%' %}
          {%- assign class_width = 'col-md-10' -%}
          {% when '100%' %}
          {%- assign class_width = 'col-md-12' -%}
          {%- endcase -%}
          {%- case block.type -%}
          {% when 'menu' %}
          <div class="container footer-menu-width {{class_width}} col-sm-6 col-xs-12 {{ block.settings.menu_position }} {{ block.settings.menu_layout }}">
            {% if block.settings.footer_heading != blank %}
            <h3 class="module-title oaki-module-title" style="color:black;">
              {% if settings.enable_multilang %}
              <h4 class="lang1 foot-heading" >{{ block.settings.footer_heading | split: '|' | first }}</h4>
              <h4 class="lang2 foot-heading" >{{ block.settings.footer_heading | split: '|' | last }}</h4>
              {% else %}
              {{ block.settings.footer_heading | split: '|' | first }}
              {% endif %}
            </h3>
            {% endif %}
            {%- assign footercol1 = block.settings.footercol1 -%}
            {% if footercol1.size > 0 %}
            <ul class="content">
              {% for link in linklists[footercol1].links %}

              <!-- <script type="text/javascript">
                console.log("{{link.title}}");
              </script> -->

              <li>
                {% if link.title contains "Address" %}

                    <script type="text/javascript">
                      console.log("I am Address");
                    </script>

                      <a href="{{ link.url }}" target="_blank">{{ link.title }}</a>

                      {% elsif link.title contains "Email" %}

                      <script type="text/javascript">
                        console.log("I am Email");
                      </script>

                      <a href="{{ link.url }}" target="_blank">{{ link.title }}</a>

                    {% else %}

                    <a href="{{ link.url }}">{{ link.title }}</a>

                {% endif %}

              </li>
              {% endfor %}
            </ul>
            {% endif %}
          </div>
          {% when 'newsletter' %}
          <div class="{{class_width}} col-sm-6 col-xs-12 footer-newsletter-width">
            <div class="home-newsletter {{ block.settings.newsletter_position }}">
              <div class="newsletter">
                {% if block.settings.home_newsletter_title != blank %}
                <div class="title_newsletter" style="color:black">
                  {% if settings.enable_multilang %}
                  <h3 class="module-title oaki-module-title" style="color:black;"></h3>
                  <span class="lang1 foot-heading">{{ block.settings.home_newsletter_title | split: '|' | first }}</span>
                  <span class="lang2 foot-heading">{{ block.settings.home_newsletter_title | split: '|' | last }}</span>
                  {% else %}
                  {{ block.settings.home_newsletter_title | split: '|' | first }}
                  {% endif %}
                </div>
                {% endif %}
                {% if block.settings.home_newsletter_subtitle != blank %}
                <div class="subtitle_newsletter" style="color:#464646">
                  {% if settings.enable_multilang %}
                  <span class="lang1">{{ block.settings.home_newsletter_subtitle | split: '|' | first }}</span>
                  <span class="lang2">{{ block.settings.home_newsletter_subtitle | split: '|' | last }}</span>
                  {% else %}
                  {{ block.settings.home_newsletter_subtitle | split: '|' | first }}
                  {% endif %}
                </div>
                {% endif %}
                <div class="content">
                  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-ajaxchimp/1.3.0/jquery.ajaxchimp.min.js"></script>
                  <form id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="input-group">
                    <div class="input-group newsletter_form_content">
                      <input  class="input__field input-group-field" style="border: 2px solid #97bd61" id="mc-email-address" type="email" value="" name="EMAIL" aria-label="Email Address" placeholder="Your email address" required>
                      <input type="submit" id="mc-subscribe-btn" class="submit-btn" value="SUBMIT" name="subscribe">
                    </div>
                  </form>
                  <script type="text/javascript">
                    var mc_subscribe_callback = function(resp) {
                      if (resp.result == 'success') {
        				alert("Successfully subscribed to oaki.com, thank you!");
					  }
                      else {
                        alert(resp.msg);
                      }
                    };
                    $('#mc-embedded-subscribe-form').ajaxChimp({
					    //url: '{{ settings.mailing_list_form_action }}',
	                   	url: "https://oaki.us7.list-manage.com/subscribe/post?u=dbacd036df81ee88e9e5ff78f&id=96d032875b",
                      	callback: mc_subscribe_callback
					});
                  </script>
                </div>
                {% if block.settings.home_newsletter_subtitle_2 != blank %}
                <div class="subtitle2_newsletter">
                  {% if settings.enable_multilang %}
                  <span class="lang1">{{ block.settings.home_newsletter_subtitle_2 | split: '|' | first }}</span>
                  <span class="lang2">{{ block.settings.home_newsletter_subtitle_2 | split: '|' | last }}</span>
                  {% else %}
                  {{ block.settings.home_newsletter_subtitle_2 | split: '|' | first }}
                  {% endif %}
                </div>
                {% endif %}
              </div>
            </div>
          </div>
          {% when 'iconsocial' %}
          <div class="{{class_width}} col-sm-6 col-xs-12 {{ block.settings.iconsocial_position }} {{ block.settings.iconsocial_layout }}">
            {% if  block.settings.footer_column_3_title != blank %}
            <h3 class="module-title oaki-module-title" style="color:black">{{ block.settings.footer_column_3_title }}</h3>
            {% endif %}
            <div class="content custom-fotter-top">
              <div class="footer-icon-share">
                {% if block.settings.footer_column_4_icon_1 != blank %}
                <a href="{{ block.settings.footer_column_4_icon_link_1 }}">
                  <i class="{{ block.settings.footer_column_4_icon_1 }}"></i>
                  <span>{{ block.settings.footer_column_4_icon_title_1 }}</span>
                </a>
                {% endif %}
                {% if block.settings.footer_column_4_icon_2 != blank %}
                <a href="{{ block.settings.footer_column_4_icon_link_2 }}">
                  <i class="{{ block.settings.footer_column_4_icon_2 }}"></i>
                  <span>{{ block.settings.footer_column_4_icon_title_2 }}</span>
                </a>
                {% endif %}
                {% if block.settings.footer_column_4_icon_3 != blank %}
                <a href="{{ block.settings.footer_column_4_icon_link_3 }}">
                  <i class="{{ block.settings.footer_column_4_icon_3 }}"></i>
                  <span>{{ block.settings.footer_column_4_icon_title_3 }}</span>
                </a>
                {% endif %}
                {% if block.settings.footer_column_4_icon_4 != blank %}
                <a href="{{ block.settings.footer_column_4_icon_link_4 }}">
                  <i class="{{ block.settings.footer_column_4_icon_4 }}"></i>
                  <span>{{ block.settings.footer_column_4_icon_title_4 }}</span>
                </a>
                {% endif %}
                {% if block.settings.footer_column_4_icon_5 != blank %}
                <a href="{{ block.settings.footer_column_4_icon_link_5 }}">
                  <i class="{{ block.settings.footer_column_4_icon_5 }}"></i>
                  <span>{{ block.settings.footer_column_4_icon_title_5 }}</span>
                </a>
                {% endif %}
              </div>
            </div>
          </div>

          {% when 'customtext' %}
          <div class="{{class_width}} col-sm-6 col-xs-12">
            {% if block.settings.footer_customtext != blank %}
            <div class="text-footerbt {{ block.settings.customtext_position }}">
              {% if settings.enable_multilang %}
              <span class="lang1">{{ block.settings.footer_customtext | split: '|' | first }}</span>
              <span class="lang2">{{ block.settings.footer_customtext | split: '|' | last }}</span>
              {% else %}
              {{ block.settings.footer_customtext | split: '|' | first }}
              {% endif %}
            </div>
            {% endif %}
          </div>
          {% when 'separator' %}
          <div class="{{ class_width }} " style="margin:{{block.settings.margin_separator }};">
            <div class="" style="border-top: {{block.settings.separator_borderwidth}} {{block.settings.settinglink_style}} {{block.settings.color_navigation}}"></div>
          </div>

          {% when 'copyright' %}
          <div class="{{class_width}} col-sm-6 col-xs-12">
            <address class="{{ block.settings.copyright_position }}">
              {% if settings.enable_multilang %}
              <span class="lang1">{{ block.settings.footer_copyright | split: '|' | first }}</span>
              <span class="lang2">{{ block.settings.footer_copyright | split: '|' | last }}</span>
              {% else %}
              {{ block.settings.footer_copyright | split: '|' | first }}
              {% endif %}
            </address>
          </div>
          {%- endcase -%}
          {% endfor %}
        </div>
      </div>
    </div>
    {% unless section.settings.enable_footer_fullwidth %}
  </div>
  {% endunless  %}


</footer>


{% include 'message-modal' %}
<style>
  {% if section.settings.enable_footer_background_image %}
  .footer-container{
    background-image:url("{{ section.settings.image_footer_background_imag | img_url: '1600x' }}");
      background-position: {{ section.settings.bg_footer_position_x }} {{ section.settings.bg_footer_position_y }};
      background-repeat: {{ section.settings.bg_footer_repeat }};
      background-attachment: {{ section.settings.bg_footer_attachment }};
      }
  {% endif %}

</style>
