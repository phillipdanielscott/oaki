/********************************

  Product and Collection page

********************************/

.product-bg, .related-w{
  background-color: white !important;
}

.out-of-stock-btn button{
  font-size: 22px;
  height: 90px;
  width: 70%;
  background-color: #97bd61;
  // aab6c3
  color: white;
  margin-top: 15px;
}


.out-of-stock-btn button,
#product-add-to-cart{
  height: 60px !important;
  font-size: 19px !important;
  border: 0px !important;
}

.variant-carousel{
  display: unset;
}

.variant-container{
    display: inline-block;
    float: left;
    border-radius: 60%;
    width: 70px;
    height: 70px;
    overflow: hidden;
    background-color: white;
    position: relative;
    border: 2px solid #97bd61;
    margin-right: 12px;
    margin-bottom: 12px;
 }

 .variant-container img{
   width: 50%;
   margin: 0 auto;
   display: block;
   position: absolute;
   top: 50%;
   left: 50%;
   transform: translate(-50%, -50%);
 }

 .oaki-variant-option-container{
   margin: 20px 0px;
 }

 .also-available-text{
   margin-bottom: 9px;
   font-weight: 600;
  }

  .oaki-variant-option-container h3:after{
    height: 2px;
    display: block;
    width: 50px;
    background: #97bd61;
    content: '';
    margin-top: 13px;
  }

  .variant-prev,
  .variant-next{
    color: #97bd61 !important;
    border: 2px solid !important;
    height: auto !important;
    border-radius: 64% !important;
    width: auto !important;
    padding: 5px 10px !important;
  }

  .variant-carousel .owl-controls .owl-buttons div{
    position: relative !important;
    top: 16px;
  }

  .contact-info-container p{
    font-size: 16px;
    text-align: center;
    padding-top: 2px;
    margin-bottom: 0px;
  }

@media only screen and (max-width:1440px){

      .contact-info-container p{
        font-size: 14px;
      }

      .contact-info-container .col-md-12,
      .contact-info-container .col-sm-12,
      .contact-info-container .col-xs-12{
        padding:0px;
      }
  }

  @media only screen and (max-width:1024px){

    .contact-info-container p{
      font-size: 10px;
    }

  }

  @media only screen and (max-width:768px){

    .contact-info-container p{
      font-size: 16px;
    }

  }

.sizing-chart-container{
