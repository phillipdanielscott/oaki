/********************************

          Product page

********************************/

.product-modal-image{
  position: absolute;
  right: 0;
  left: 0;
  margin: auto;
  top: 0;
  bottom: 0;
}

.modal-x{
  color: white;
  position: absolute;
  right: 28%;
  font-size: 25px;
  bottom: 78%;
  padding: 10px 12px;
  border: 2px solid white;
  border-radius: 60%;
}

 .modal-x:hover{
   background-color: #97BD66;
   transition: 0.1s;
 }

@media only screen and (max-width:1440px){
  .modal-x{
    right: 11%;
    bottom: 82%;
  }

  .product-modal-image{
    max-width: 75%;
  }
}

@media only screen and (max-width:1024px){

  #add-to-cart-form span.up-qty{
        margin-top: 0px;
  }

   #add-to-cart-form span.up-qty .clever-icon-up{
         line-height: unset;
   }

   .variant-container{
     width: 60px;
     height: 60px;
   }

   .variant-carousel .owl-item{
     margin-bottom: 20px;
     margin-right: 10px;
   }
}

@media only screen and (max-width:768px){

  .product-shop .product-title.has-btn{
    padding-right: 0px;
    text-align: center;
    margin-top: 20px;
  }

  .shop-detail-content .product-title h1 .lang1:after{
    margin: 0 auto;
    margin-top: 15px;
  }

  .sizing-chart-container .panel-title{
    text-align: center;
  }

  .sizing-chart-container{
    width: 70%;
    margin: 0 auto;
    margin-bottom: 60px;
  }

  .product-shop .prices{
    text-align: center;
    float: none;
  }

  .swatch-container{
    width: 80% !important;
    margin: 0 auto;
  }

  .prices-wr{
    margin-bottom: 0px;
    border: 0px;
  }

  .swatch input:checked + label, .swatch label:hover{
    display: block;
    width: 80%;
    margin: 0 auto;
    float: none;
    height: 50px !important;
  }

  .swatch label{
    display: block;
    width: 80%;
    margin: 0 auto;
    float: none;
    height: 50px !important;
  }

  .out-of-stock-btn button,
  #product-add-to-cart{
    margin: 0 auto;
    display: block;
    margin-bottom: 30px;
  }

  .variant-container{
    display: block;
    margin: 0 auto;
    float: none;
  }

  .also-available-text{
    text-align: center;
  }

  .oaki-variant-option-container h3:after{
    margin: 0 auto;
    margin-top: 8px;
  }

  .modal-x{
    bottom: 69%;
  }

}

@media only screen and (max-width:450px){

  .variant-carousel .owl-item{
